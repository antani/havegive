<h1>Listing wants</h1>

<table>
  <tr>
    <th>User</th>
    <th>Want string</th>
    <th>Want category</th>
    <th>Want place</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wants.each do |want| %>
  <tr>
    <td><%= want.user_id %></td>
    <td><%= want.want_string %></td>
    <td><%= want.want_category %></td>
    <td><%= want.want_place %></td>
    <td><%= link_to 'Show', want %></td>
    <td><%= link_to 'Edit', edit_want_path(want) %></td>
    <td><%= link_to 'Destroy', want, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Want', new_want_path %>
